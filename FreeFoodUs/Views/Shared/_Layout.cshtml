<body>
    <!-- Start Main Navigation -->
    <div class="outter-wrapper header-container">
        <div class="wrapper">
            <header class="clearfix">
                <a href="/" class="logo">
                    <img src="/img/logo@2x.png" alt="Free Food" /></a>
                <nav id="nav-wrap">
                    <ul class="nav" id="nav">
                        <li class="nav-parent"><a class="active" href="/">Home</a></li>
                        <li class="nav-parent"><a href="#">Donate</a>
                            <ul>
                                <li><a href="/donate/">Buy a meal</a></li>
                                <li><a href="/donate/food">Supermarkets</a></li>
                                <li><a href="/donate/restaurants">Restaurants</a></li>
                                <li><a href="/donate/volunteer">Volunteer</a></li>
                            </ul>
                        </li>
                        <li class="last donate"><a href="/food">Get Food</a></li>
                    </ul>
                </nav>
            </header>
        </div>
    </div>
    <!-- End Main Navigation -->
    <!-- Start Outter Wrapper / Main Container / wrapper -->
    <div class="outter-wrapper">
        <div class="main-container">
            <div class="main wrapper clearfix">
                <!-- Start Main Content -->
                <div class="main-content">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <!-- Start Footer -->
    <footer class="outter-wrapper feature">
        <div class="wrapper">
            <div class="clearfix">
                <div class="foot right">
                    <!-- Social Media icons -->
                    <ul class="social-link">
                        <li><a href="#"><em class="icon-facebook-sign"></em></a></li>
                        <li><a href="#"><em class="icon-twitter-sign"></em></a></li>
                        <li><a href="#"><em class="icon-tumblr-sign"></em></a></li>
                        <li><a href="#"><em class="icon-google-plus-sign"></em></a></li>
                    </ul>
                </div>
                <!-- Copyright Info -->
                <div class="foot left">&copy; Copyright Dave & Marco 2013</div>
            </div>
        </div>
    </footer>
    <!-- End Footer -->
